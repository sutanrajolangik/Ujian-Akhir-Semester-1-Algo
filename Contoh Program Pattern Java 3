public class Main
{
	public static void StarRightTriangle(int n){
	   int a , b;
	   for(a = 0; a < n; a++)
	   {
	       for (b = 0; b <= a; b++)
	       {
	           System.out.print("Indonesia");
	       
	       }
	       System.out.println();
	   }
     		
	} 
    public static void main(String args[])
    {
           int k = 10;
           StarRightTriangle(k);
    }
}
